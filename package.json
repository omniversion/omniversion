{
  "name": "omniversion",
  "version": "0.44.0",
  "description": "Omniversion",
  "main": "index.js",
  "directories": {
    "doc": "docs",
    "test": "cli/test"
  },
  "goBinary": {
    "name": "omniversion",
    "path": "./bin",
    "url": "https://github.com/omniversion/omniversion-cli/releases/download/v{{version}}/npm_omniversion_{{version}}_{{platform}}_{{arch}}.tar.gz"
  },
  "files": [],
  "scripts": {
    "test": "cd cli && go test ./... -coverprofile=test/coverage.out && cd ../python && python3 -m pytest --cov=omniversion --cov-report=xml test_omniversion",
    "docs": "rm -r docs/python && cd python && pdoc omniversion --html --output-dir ../docs/python",
    "postinstall": "go-npm install",
    "preuninstall": "go-npm uninstall"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omniversion/omniversion-cli.git"
  },
  "keywords": [
    "omniversion"
  ],
  "author": "Layer9 GmbH",
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/omniversion/omniversion-cli/issues"
  },
  "homepage": "https://github.com/omniversion/omniversion-cli#readme",
  "dependencies": {
    "@gzuidhof/go-npm": "^0.1.13"
  }
}
